<body>
    <header class="navbar navbar-expand-sm navbar-dark bg-dark-blue">
        <div class="navbar-brand mr-2">
            <button class="btn btn-outline-warning w-100"><i class="fas fa-user-circle"></i> Customer Service
                Platform</button>
        </div>
    </header>
    <div class="jumbotron jumbotron-fluid">
        <div class="container">
            <div class="text-center">
                <p class="display-1">
                    <i class="fas fa-user-circle"></i>
                </p>
                <p class="lead">Customer Service Platform</p>
            </div>
            <div class="form-row form-group  justify-content-center">
                <button class="btn btn-info mx-2 cmd" data-cmd="modal" data-type="login">
                    <i class="fas fa-sign-in-alt"></i> Login
                </button>
                <button class="btn btn-secondary mx-2 cmd" data-cmd="modal" data-type="register">
                    <i class="fas fa-user-plus"></i> Register
                </button>
            </div>
        </div>
    </div>
    <!-- Modal -->
    <div class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header bg-dark-blue">
                </div>
                <div class="modal-body text-center">
                    <div class="container pl-3">
                    </div>
                </div>
                <div class="modal-footer">
                </div>
            </div>
        </div>
    </div>

